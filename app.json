{
  "name": "Testing-Bot",
  "description": "A Simple Testing Telegram Bot Written in Python Using Pyrogram Library.",
  "repository": "https://github.com/coixo/TestingBot",
  "logo": "https://example.com/path-to-your-logo.png", 
  "stack": "container",
  "env": {
    "APP_ID": {
      "description": "The ID for the app",
      "value": "your_app_id_here", 
      "required": true
    },
    "HASH_ID": {
      "description": "The hash ID for the app",
      "value": "your_hash_id_here", 
      "required": true
    },
    "TOKEN": {
      "description": "Your authentication token",
      "value": "your_token_here", 
      "required": true
    }
  },
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "basic"
    }
  }
}
